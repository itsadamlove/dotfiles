{
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "php-cs-fixer.rules": "@PSR13,ordered_imports,no_unused_imports",
  "php-cs-fixer.config": ".php_cs",
  "intelephense.licenceKey": "enterkeyhere",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript", "html", "javascript-jasmine-arrow"],
    "typescriptreact": ["javascript", "html", "javascript-jasmine-arrow"],
    "typescript": ["javascript", "html", "javascript-jasmine-arrow"],
    "edge": ["html"],
    "liquid": ["html"],
    "scss": ["css"],
    "sass": ["css"]
  }
}
