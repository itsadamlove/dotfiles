{
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "coc.preferences.formatOnSaveFiletypes": ["*", "prisma"],
  "php-cs-fixer.rules": "@PSR12,ordered_imports,no_unused_imports",
  "php-cs-fixer.toolPath": "~/.config/coc/extensions/coc-php-cs-fixer-data/php-cs-fixer",
  "intelephense.licenceKey": "enterkeyhere",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript", "html", "javascript-jasmine-arrow"],
    "typescriptreact": ["javascript", "html", "javascript-jasmine-arrow"],
    "typescript": ["javascript", "html", "javascript-jasmine-arrow"],
    "edge": ["html"],
    "liquid": ["html"],
    "scss": ["css"],
    "sass": ["css"]
  }
}
